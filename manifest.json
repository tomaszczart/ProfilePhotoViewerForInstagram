{
  "name": "Profile Photo Viewer for Instagram",
  "version": "1.1.0",
  "description": "__MSG_chrome_extension_description__",
  "default_locale": "en",
  "icons": { 
  "128": "icon128.png",
  "256": "icon256.png"
  },
  "permissions": [
    "tabs",
    "https://www.instagram.com/*"
  ],
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
    "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "css": ["style.css"],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 2
}